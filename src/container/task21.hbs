



<main>
        <div class="card">
        <div class="card-body">
            {{#with heading}}
                {{#with main}}
                <h1>{{title}}</h1>
                <p>{{text}}</p>
                <div>
                    {{#with button_text}}
                    <button class="btn btn-primary">{{solid}}</button>
                    <br>
                    <br>
                    <button class="btn btn-outline-primary">{{outline}}</button>
                    {{/with}}
                </div>
                {{/with}}

                <div>
                </div>
        </div>
        </div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with second}}
                        <h2>{{title}}</h2>
                        <p>{{text}}</p>
                        <div>
                            {{#with button_text}}
                                <button class="btn btn-secondary">{{solid}}</button>
                                <br>
                                <br>
                                <button class="btn btn-outline-secondary">{{outline}}</button>
                            {{/with}}
                        </div>
                    {{/with}}
                </div>
            </div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with subblock}}
                        <h3>{{title}}</h3>
                        <p>{{text}}</p>
                        <div>
                            {{#with button_text}}
                            <button class="btn btn-dark">{{solid}}</button>
                            <br>
                            <br>
                            <button class="btn btn-outline-dark">{{outline}}</button>
                            {{/with}}
                        </div>
                    {{/with}}
                </div>
            </div>
            {{/with}}
            <br>
            <div class="card">
            <div class="card-body">
                {{#with nested_display}}
                    <div class="display-1">{{title}}</div>
                    <div class="card">
                        <div class="card-body">
                            {{#with children_display}}
                                <div class="display-5">{{title}}</div>
                                <p>{{text}}</p>
                                    {{#with button}}
                                    <button class="btn btn-outline-warning">{{text}}</button>
                                    {{/with}}
                            {{/with}}
                        </div>
                    </div>
                    <br>
                    <div>
                        <h6>{{header_text}}</h6>
                        <button class="btn btn-info">{{button.text}}</button>
                    </div>
                    
                {{/with}}
            </div>
            </div>
            <br>
        <div class="card">
            <div class="card-body">
                {{#with paragraph}}
                    <p class="text-uppercase">{{title}}</p>
                    <p class="text-lowercase">{{text}}</p>
                    {{#with button}}
                        <a type="button" class="btn btn-link" href="https://github.com/" role="button">{{text}}</a>
                    {{/with}}
                
            </div>
            <div class="card">
                <div class="card-body">
                    {{#each list_name}}
                        <ul>
                            <li>
                                <a href="https://github.com/">{{text}}</a>
                            </li>
                            
                        </ul>
                    {{/each}}
                </div>
                {{/with}}
            </div>
        </div>
    
    
</main>